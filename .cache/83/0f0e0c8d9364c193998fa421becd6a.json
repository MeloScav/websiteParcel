px;\r\n}\r\n\r\nbody {\r\n  font-family: Arial, Helvetica, sans-serif;\r\n}\r\n\r\na {\r\n  text-decoration: none;\r\n}\r\na:link,\r\na:visited {\r\n  color: #000;\r\n}\r\n","..\\src\\sass\\layout\\_navbar.scss":".container {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  max-width: calculateRem(960);\r\n  width: 100%;\r\n}\r\n\r\n// NavBar\r\n.navbar {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  display: flex;\r\n  justify-content: center;\r\n  min-height: $height-nav;\r\n  background-color: transparent;\r\n  z-index: 9;\r\n  transition: all 0.3s ease-out;\r\n}\r\n\r\n// LOGO\r\n.logo-container {\r\n  position: relative;\r\n  width: 8rem;\r\n  height: auto;\r\n  display: flex;\r\n  align-items: center;\r\n  transition: all 0.3s ease-out;\r\n}\r\n.logo {\r\n  position: absolute;\r\n  display: block;\r\n  top: 0;\r\n  left: 0;\r\n  opacity: 0;\r\n  transition: all 0.3s ease-out;\r\n\r\n  .logo-img {\r\n    max-height: 100%;\r\n    max-width: 100%;\r\n  }\r\n}\r\n\r\n.logo-container .logo:nth-child(1) {\r\n  opacity: 1;\r\n}\r\n\r\n// LINKS\r\n.nav-links {\r\n  display: flex;\r\n  flex: 1;\r\n  justify-content: space-around;\r\n  align-items: center;\r\n  list-style: none;\r\n  // height: 100%;\r\n\r\n  @media screen and(max-width:768px) {\r\n    position: fixed;\r\n    flex-direction: column;\r\n    opacity: 0;\r\n    pointer-events: none;\r\n    background-color: $white;\r\n    min-height: 100vh;\r\n    width: 100%;\r\n  }\r\n  &.open {\r\n    display: flex;\r\n    opacity: 1;\r\n    pointer-events: initial;\r\n  }\r\n  &.open li {\r\n    opacity: 1;\r\n    pointer-events: initial;\r\n  }\r\n  li {\r\n    transition: all 0.3s ease-in-out;\r\n    @media screen and(max-width:768px) {\r\n      opacity: 0;\r\n      pointer-events: none;\r\n    }\r\n  }\r\n  a {\r\n    color: $white;\r\n    font-size: calculateRem(20);\r\n    @media screen and(max-width:768px) {\r\n      color: $black;\r\n    }\r\n  }\r\n}\r\n\r\n//Links transitions\r\n.nav-links li:nth-child(1) {\r\n  transition-delay: 0.2s;\r\n}\r\n.nav-links li:nth-child(2) {\r\n  transition-delay: 0.4s;\r\n}\r\n.nav-links li:nth-child(3) {\r\n  transition-delay: 0.6s;\r\n}\r\n.nav-links li:nth-child(4) {\r\n  transition-delay: 0.8s;\r\n}\r\n\r\n.container-content {\r\n  height: 90vh;\r\n}\r\n\r\n// Only in responsive\r\n@media screen and(max-width:768px) {\r\n  // Hamburger\r\n  .hamburger {\r\n    position: absolute;\r\n    right: 5%;\r\n    top: 50%;\r\n    transform: translate(-5%, -50%);\r\n    cursor: pointer;\r\n    z-index: 2;\r\n  }\r\n  .line {\r\n    margin: calculateRem(5);\r\n    width: calculateRem(30);\r\n    height: calculateRem(3);\r\n    background-color: $white;\r\n  }\r\n\r\n  // Animation Open\r\n  .hamburger.open .line {\r\n    -webkit-transition: 0.3s ease-in-out;\r\n    -moz-transition: 0.3s ease-in-out;\r\n    -o-transition: 0.3s ease-in-out;\r\n    transition: 0.3s ease-in-out;\r\n    background-color: $black;\r\n  }\r\n  .hamburger.open .line:nth-child(1) {\r\n    transform: rotate(50deg) translate(15%);\r\n  }\r\n  .hamburger.open .line:nth-child(2) {\r\n    display: none;\r\n  }\r\n  .hamburger.open .line:nth-child(3) {\r\n    transform: rotate(-50deg) translate(15%);\r\n  }\r\n  // Animation Close\r\n  .hamburger .line {\r\n    -webkit-transition: 0.3s ease-in-out;\r\n    -moz-transition: 0.3s ease-in-out;\r\n    -o-transition: 0.3s ease-in-out;\r\n    transition: 0.3s ease-in-out;\r\n  }\r\n  .hamburger .line:nth-child(1) {\r\n    transform: rotate(0deg) translate(0%);\r\n  }\r\n  .hamburger .line:nth-child(2) {\r\n    display: block;\r\n  }\r\n  .hamburger .line:nth-child(3) {\r\n    transform: rotate(0deg) translate(0%);\r\n  }\r\n}\r\n\r\n// SCROLL\r\n.header.scroll-change .navbar {\r\n  background-color: $white;\r\n  box-shadow: 0px 5px 5px rgba($black, 0.3);\r\n  min-height: calc(#{$height-nav} - 3.5vh);\r\n  a {\r\n    font-size: calculateRem(17);\r\n    color: #00818a;\r\n  }\r\n}\r\n\r\n.header.scroll-change .logo-container .logo:nth-child(1) {\r\n  opacity: 0;\r\n}\r\n.header.scroll-change .logo-container .logo:nth-child(2) {\r\n  opacity: 1;\r\n  width: 3rem;\r\n  top: 50%;\r\n  left: 0%;\r\n  transform: translate(0%, -50%);\r\n}\r\n\r\n.header.scroll-change .line {\r\n  background-color: $black;\r\n}\r\n","..\\src\\sass\\helpers\\_variables.scss":"// Color\r\n$white: #fff;\r\n$black: #000;\r\n\r\n// Height\r\n$height-nav: 13vh;\r\n","..\\src\\sass\\pages\\_home.scss":"// BANNER\r\n.banner {\r\n  .gizmo-img {\r\n    width: 100%;\r\n    height: 100vh;\r\n    -o-object-fit: cover;\r\n    object-fit: cover;\r\n    z-index: 0;\r\n  }\r\n}\r\n\r\n// TITLE\r\n.divTitle {\r\n  margin: calculateRem(40) 0 calculateRem(30) 0;\r\n  text-align: center;\r\n  h2 {\r\n    margin-top: calculateRem(5);\r\n    font-size: calculateRem(18);\r\n    font-weight: normal;\r\n    font-style: italic;\r\n  }\r\n}\r\n\r\n// BOX\r\n.containerBox {\r\n  padding-top: calculateRem(20);\r\n  display: flex;\r\n  flex-direction: row;\r\n  flex-wrap: wrap;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n.box {\r\n  margin: 0 calculateRem(10) 0 calculateRem(10);\r\n  padding: calculateRem(15);\r\n  width: calculateRem(260);\r\n  height: calculateRem(260);\r\n  border: 1px solid #d68524;\r\n  border-radius: calculateRem(20);\r\n\r\n  a {\r\n    width: 100%;\r\n    height: 100%;\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n  }\r\n  p {\r\n    font-style: italic;\r\n  }\r\n  button {\r\n    padding: 10px 20px 10px 20px;\r\n    background-color: #53bec6;\r\n\r\n    font-weight: bold;\r\n    border: 1px solid #53bec6;\r\n    border-radius: 8px;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"14a2840cdda2e49c8c67a1c987181946","cacheData":{"env":{}}}