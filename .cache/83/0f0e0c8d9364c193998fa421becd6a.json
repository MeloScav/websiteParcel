r\n}\r\n\r\n// LINKS\r\n.nav-links {\r\n  display: flex;\r\n  flex: 1;\r\n  justify-content: space-around;\r\n  align-items: center;\r\n  list-style: none;\r\n  // height: 100%;\r\n\r\n  @media screen and(max-width:768px) {\r\n    position: fixed;\r\n    flex-direction: column;\r\n    opacity: 0;\r\n    pointer-events: none;\r\n    background-color: $white;\r\n    min-height: 100vh;\r\n    width: 100%;\r\n  }\r\n  &.open {\r\n    display: flex;\r\n    opacity: 1;\r\n    pointer-events: initial;\r\n  }\r\n  &.open li {\r\n    opacity: 1;\r\n    pointer-events: initial;\r\n  }\r\n  li {\r\n    transition: all 0.3s ease-in-out;\r\n    @media screen and(max-width:768px) {\r\n      opacity: 0;\r\n      pointer-events: none;\r\n    }\r\n  }\r\n  a {\r\n    color: $white;\r\n    font-size: 20px;\r\n    @media screen and(max-width:768px) {\r\n      color: $black;\r\n    }\r\n  }\r\n}\r\n\r\n//Links transitions\r\n.nav-links li:nth-child(1) {\r\n  transition-delay: 0.2s;\r\n}\r\n.nav-links li:nth-child(2) {\r\n  transition-delay: 0.4s;\r\n}\r\n.nav-links li:nth-child(3) {\r\n  transition-delay: 0.6s;\r\n}\r\n.nav-links li:nth-child(4) {\r\n  transition-delay: 0.8s;\r\n}\r\n\r\n// li.fade {\r\n//   opacity: 1;\r\n//   transition: all 0.35s ease-in-out;\r\n// }\r\n\r\n.container-content {\r\n  height: 90vh;\r\n}\r\n\r\n// Only in responsive\r\n@media screen and(max-width:768px) {\r\n  // Hamburger\r\n  .hamburger {\r\n    position: absolute;\r\n    right: 5%;\r\n    top: 50%;\r\n    transform: translate(-5%, -50%);\r\n    cursor: pointer;\r\n    z-index: 2;\r\n  }\r\n  .line {\r\n    margin: 5px;\r\n    width: 30px;\r\n    height: 3px;\r\n    background-color: #000;\r\n  }\r\n\r\n  // Animation Open\r\n  .hamburger.open .line {\r\n    -webkit-transition: 0.3s ease-in-out;\r\n    -moz-transition: 0.3s ease-in-out;\r\n    -o-transition: 0.3s ease-in-out;\r\n    transition: 0.3s ease-in-out;\r\n  }\r\n  .hamburger.open .line:nth-child(1) {\r\n    transform: rotate(50deg) translate(15%);\r\n  }\r\n  .hamburger.open .line:nth-child(2) {\r\n    display: none;\r\n  }\r\n  .hamburger.open .line:nth-child(3) {\r\n    transform: rotate(-50deg) translate(15%);\r\n  }\r\n  // Animation Close\r\n  .hamburger .line {\r\n    -webkit-transition: 0.3s ease-in-out;\r\n    -moz-transition: 0.3s ease-in-out;\r\n    -o-transition: 0.3s ease-in-out;\r\n    transition: 0.3s ease-in-out;\r\n  }\r\n  .hamburger .line:nth-child(1) {\r\n    transform: rotate(0deg) translate(0%);\r\n  }\r\n  .hamburger .line:nth-child(2) {\r\n    display: block;\r\n  }\r\n  .hamburger .line:nth-child(3) {\r\n    transform: rotate(0deg) translate(0%);\r\n  }\r\n}\r\n\r\n// SCROLL\r\n.header.scroll-change .navbar {\r\n  background-color: $white;\r\n  box-shadow: 0px 5px 5px rgba($black, 0.3);\r\n  min-height: calc(#{$height-nav} - 3vh);\r\n  a {\r\n    font-size: 16px;\r\n    color: $black;\r\n  }\r\n}\r\n\r\n.header.scroll-change .logo {\r\n  width: 6rem;\r\n}\r\n","..\\src\\sass\\helpers\\_variables.scss":"// Color\r\n$white: #fff;\r\n$black: #000;\r\n\r\n// Height\r\n$height-nav: 13vh;\r\n","..\\src\\sass\\pages\\_home.scss":".banner {\r\n  .gizmo-img {\r\n    // margin-top: $height-nav;\r\n    width: 100%;\r\n    // height: calc(100vh - #{$height-nav});\r\n    height: 100vh;\r\n    -o-object-fit: cover;\r\n    object-fit: cover;\r\n    z-index: 0;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"c0b7302e3532c99a2f5e267649c59238","cacheData":{"env":{}}}